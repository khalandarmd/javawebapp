pipeline{
  
  agent {
  label 'mvn'
}

  stages{
    stage('Checkout'){
      steps{
        git branch: 'main', credentialsId: 'GitHub-Cred-2023', url: 'https://github.com/khalandarmd/javawebapp.git'
      }
    }
    stage('Build'){
      steps{
        sh 'mvn clean package'
      }
    }
  }
}
